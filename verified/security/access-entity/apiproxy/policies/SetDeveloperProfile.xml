<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables continueOnError="false" enabled="true" name="SetDeveloperProfile">
    <DisplayName>SetDeveloperProfile</DisplayName>
    <Source>AccessEntity.GetDeveloperProfile</Source>
    <VariablePrefix>developer</VariablePrefix>
    <XMLPayload>
        <Variable name="email" type="string">
            <!-- You parse elements from the developer profile using XPath. -->
            <XPath>/Developer/Email</XPath>
        </Variable>
    </XMLPayload>
</ExtractVariables>