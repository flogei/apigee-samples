<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>JS-EncodeAuthHeader</Name>
            </Step>
        </Request>
        <Response>
            <Step>
                <Name>AM-EchoVariables</Name>
            </Step>
        </Response>
    </PreFlow>
    <Flows/>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <HTTPProxyConnection>
        <BasePath>/apigee-samples/js-base64encoder</BasePath>
    </HTTPProxyConnection>
    <RouteRule name="default"/>
</ProxyEndpoint>